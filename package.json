{
  "homepage": "https://ryantremarco.github.io",
  "name": "tremarco",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.7.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.2",
    "web-vitals": "^1.1.0"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "css-build": "node-sass --omit-source-map-url src/mystyles.scss public/mystyles.css",
    "css-watch": "npm run css-build -- --watch",
    "start": "npm run css-build && react-scripts start",
    "build": "npm run css-build && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "bulma": "^0.9.2",
    "bulmaswatch": "^0.8.1",
    "gh-pages": "^3.1.0",
    "node-sass": "^5.0.0",
    "react-router-hash-link": "^2.4.0",
    "react-test-renderer": "^17.0.1"
  }
}
